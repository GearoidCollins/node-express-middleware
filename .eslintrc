{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "mocha": true,
    "protractor": true,
    "jquery": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "CT": true,
    "CTSCOPE": true,
    "Microsoft": true,
    "angular": true,
    "debug": true,
    "google": true,
    "setTranslations": true,
    "DocumentTouch": true,
    "expect": true,
    "assert": true,
    "module": true,
    "inject": true,
    "ngModule": true,
    "mock": true,
    "ENV": true
  },
  "rules": {
    "keyword-spacing": 1,
    "array-bracket-spacing": [1, "never"],
    "object-curly-spacing": [1, "always"],
    "space-before-function-paren": [1, {
      "anonymous": "always",
      "named": "never"
    }],
    "space-before-blocks": 1,
    "comma-spacing": 2,
    "computed-property-spacing": 2,
    "key-spacing": 2,
    "semi-spacing": 1,
    "semi": [2, "always"],
    "no-spaced-func": 2,
    "no-whitespace-before-property": 2,
    "camelcase": 2,
    "comma-style": [2, "last"],
    "no-console": 1,
    "no-duplicate-case": 2,
    "no-mixed-spaces-and-tabs": 2,
    "indent": [1, 2, {
      "SwitchCase": 1
    }],
    "quotes": [2, "single"],
    "complexity": [1, 12],
    "max-statements": [1, 24],
    "max-depth": [1, 5],
    "max-len": [1, 140],
    "max-lines": [1, {
      "max": 300,
      "skipBlankLines": true,
      "skipComments": true
    }],
    "max-nested-callbacks": [1, 3],
    "no-unused-vars": 2,
    "curly": 2,
    "no-empty": [2, {
      "allowEmptyCatch": true
    }],
    "arrow-parens": 1,
    "angular/module-getter": 0,
    "angular/no-service-method": 0,
    "newline-after-var": 1,
    "space-in-parens": 1,
    "no-var": 2
  }
}
